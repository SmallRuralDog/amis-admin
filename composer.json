        }
    ],
    "homepage": "https://github.com/SmallRuralDog/amis-admin",
    "keywords": [
        "Laravel",
        "AmisAdmin"
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "SmallRuralDog\\AmisAdmin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SmallRuralDog\\AmisAdmin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmallRuralDog\\AmisAdmin\\AmisAdminServiceProvider"
            ],
            "aliases": {
                "AmisAdmin": "SmallRuralDog\\AmisAdmin\\Facades\\AmisAdmin"
            }
        }
    }
}
